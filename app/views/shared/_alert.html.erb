<% if notice %>
<br>
<div class='container'>
<div class="alert alert-dark alert-dismissible fade show" role="alert">
  <%= flash[:notice] %>
   <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
</div>
<% end %>